[ req ]
default_bits = 2048
default_keyfile = key.pem
default_md = sha256
distinguished_name = subject
string_mask = utf8only
req_extensions = req_ext
prompt = no

[ subject ]
countryName = AU
stateOrProvinceName = New South Wales
localityName = Sydney
organizationName = VODAFONE HUTCHISON AUSTRALIA PTY LIMITED
organizationalUnitName = IP Networks
commonName = vim004vp002400hapxy.vodafone.com.au
#emailAddress = arshad.Masih@vodafone.com.au

[ x509_ext ]
subjectKeyIdentifier = hash
authorityKeyIdentifier = keyid,issuer
basicConstraints = CA:TRUE
keyUsage = digitalSignature, keyEncipherment
subjectAltName = @alternate_names
nsComment = "OpenSSL Generated Certificate"

[ req_ext ]
subjectKeyIdentifier = hash
basicConstraints = CA:FALSE
keyUsage = nonRepudiation, digitalSignature, keyEncipherment
extendedKeyUsage = serverAuth, clientAuth
subjectAltName = @alternate_names
nsComment = "OpenSSL Generated Certificate"

[ alternate_names ]
DNS.1 = vim004vp002400hapxy.vodafone.com.au



openssl req -out vim004vp002400hapxy.csr -newkey rsa:2048 -nodes -keyout vim004vp002400hapxy.key -config vim004vp002400hapxy_config.txt

